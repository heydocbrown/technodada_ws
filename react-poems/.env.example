# DadaCat Pipeline Configuration

# Set to 'true' to use the pipeline directly from the browser
# Set to 'false' to use the Express API proxy (recommended for production)
VITE_USE_DIRECT_PIPELINE=false

# API endpoint for fallback/proxy mode
VITE_API_ENDPOINT=http://localhost:8080/api/dadacat

# Pipeline configuration (optional)
VITE_PIPELINE_TIMEOUT=30000
VITE_PIPELINE_RETRY_COUNT=3